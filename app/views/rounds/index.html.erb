<h1>Listing rounds</h1>

<table>
  <tr>

    <th>Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rounds.each do |round| %>
  <tr>

    <td><%= round.number %></td>
    <td><%= link_to 'Show', round %></td>
    <td><%= link_to 'Edit', edit_round_path(round) %></td>
    <td><%= link_to 'Destroy', round, confirm: 'Are you sure?', method: :delete %></td>
    <td><%= link_to "Populate Expenses",new_round_expense_path(:round_id=>round.id) %></td>
    <td><%= link_to "Populate Investments",new_round_investment_path(:round_id=>round.id) %></td>
    <td><%= link_to "Populate Environmental Parameters",new_round_environmental_parameter_path(:round_id=>round.id) %></td>
    <td><%= link_to "Populate Loan Details",new_round_loan_path(:round_id=>round.id) %></td>



  </tr>
<% end %>
</table>

<br />




